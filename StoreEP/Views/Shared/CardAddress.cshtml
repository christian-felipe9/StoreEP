@model Endereco
<div class="demo-card-event mdl-card mdl-shadow--4dp mdl-cell" style="margin:10px">
    <div class="mdl-card__title mdl-card--expand">
        <h4>
            @Model.Rua
            @Model.Numero<br>
            @Model.Bairro<br>
            @Model.Cidade<br>
            @Model.Estado<br>
        </h4>
    </div>
    <div class="mdl-card__actions mdl-card--border">
        <label id="@Model.ID" class="mdl-icon-toggle mdl-js-icon-toggle mdl-js-ripple-effect" for="icon-toggle-@Model.ID">
            <input type="checkbox" id="endereco-id-@Model.ID" class="mdl-icon-toggle__input" value="@Model.ID">
            <i class="mdl-icon-toggle__label material-icons">send</i>
        </label>
        <a asp-action="Apagar" asp-controller="Endereco" class="mdl-button mdl-js-button mdl-button--icon" style="float:right" asp-route-enderecoid="@Model.ID">
            <i class="material-icons">delete_forever</i>
        </a>
    </div>
</div>
